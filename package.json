{
  "name": "shippo-1",
  "version": "1.0.0",
  "description": "**Edit a file, create a new file, and clone from Bitbucket in under 2 minutes**",
  "main": "./main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "fedex_start": "node src/js/fedex/app.js",
    "ups_start": "node src/js/ups/upsAPI.js",
    "selenium_run": "python3 $SHIPPO_HOME/src/test/selenium_fedex_test.py",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "dmg": "electron-installer-dmg ./out/myapp-darwin-x64/myapp.app ./out/myapp"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "electron": "15.4.0",
    "electron-installer-dmg": "^3.0.0"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.18.2",
    "nunjucks": "^3.2.4"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "electron_quick_start"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin",
            "windows"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
